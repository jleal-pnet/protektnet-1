<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="censere_customs_stock_move_form" model="ir.ui.view">
        <field name="name">censere_customs.stock_move_form</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position="after">
                <field name="needs_lots"/>
                <field name="finished_lots_exist"/>
                <field name="active_move_line_ids"/>
            </xpath>
            <xpath expr="//field[@name='purchase_line_id']" position="attributes">
                <attribute name="attrs">{}</attribute>
            </xpath>
        </field>
    </record>
</odoo>